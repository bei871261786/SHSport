<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <!--<solid android:color="@color/standard_textcolor_c6" />-->
            <solid android:color="@color/dingdan_background_gray" />
            <corners android:radius="@dimen/dp_size_8dp" />
            <stroke android:color="@color/border_gray" android:width="1dp"></stroke>
        </shape>

    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/homeblue" />
            <corners android:radius="@dimen/dp_size_8dp" />
            <stroke android:color="@color/border_gray"  android:width="1dp"></stroke>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <!--<solid android:color="@color/standard_textcolor_c6" />-->
            <solid android:color="@color/dingdan_background_gray" />
            <corners android:radius="@dimen/dp_size_8dp" />
            <stroke android:color="@color/border_gray"  android:width="1dp"></stroke>
        </shape>

    </item>
</selector>