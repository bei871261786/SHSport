<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:minHeight="@dimen/dp_size_60dp"
    android:orientation="horizontal">


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        android:gravity="center"
        android:paddingLeft="@dimen/dp_size_5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/jiangji_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text="奖级"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_size_14sp_T14" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:gravity="center"
                android:text="基本"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_size_13sp_T13" />

            <TextView
                android:id="@+id/zuijia_tv"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:gravity="center"
                android:text="追加"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_size_13sp_T13" />
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        android:orientation="vertical">

        <TextView
            android:id="@+id/zhongjiang_zhushu1_tv"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:gravity="center"
            android:text="注数"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_size_14sp_T14" />

        <TextView
            android:id="@+id/zhongjiang_zhushu2_tv"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:gravity="center"
            android:text="注数"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_size_14sp_T14" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        android:paddingRight="@dimen/dp_size_5dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/jiangjin_item1_tv"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="奖金(元)"
            android:textColor="@color/select_red"
            android:textSize="@dimen/sp_size_14sp_T14" />

        <TextView
            android:id="@+id/jiangjin_item2_tv"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="奖金(元)"
            android:textColor="@color/select_red"
            android:textSize="@dimen/sp_size_14sp_T14" />
    </LinearLayout>

</LinearLayout>