<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:background="@drawable/white_white_corner_shape"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40.0dip"
        android:background="@color/expandtback_red"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bqc_dialog_hos_rank"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10.0dip"
                android:text="[主]"
                android:textColor="@color/white"
                android:textSize="@dimen/standard_textsize_f6" />

            <TextView
                android:id="@+id/bqc_dialog_homename"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="队名"
                android:textColor="@color/white"
                android:textSize="@dimen/standard_textsize_f4_S" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="VS"
            android:textColor="@color/white"
            android:textSize="@dimen/standard_textsize_f4" />

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bqc_dialog_visitname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10.0dip"
                android:text="队名"
                android:textColor="@color/white"
                android:textSize="@dimen/standard_textsize_f4_S" />

            <TextView
                android:id="@+id/bqc_dialog_visit_rankg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="[客]"
                android:textColor="@color/white"
                android:textSize="@dimen/standard_textsize_f6" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="wrap_content"
        android:layout_height="1px"
        android:background="@color/black" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10.0dip"
        android:layout_marginTop="10dp"
        android:paddingLeft="16.0dip"
        android:paddingRight="30.0dip"
        android:text="@string/bqcdialog_description"
        android:textColor="@color/black"
        android:textSize="@dimen/standard_textsize_f5" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dip"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:background="@color/gray"
        android:paddingBottom="10dip"
        android:paddingLeft="6dip"
        android:paddingRight="6dip"
        android:paddingTop="10dip">

        <View
            android:layout_width="0.5dip"
            android:layout_height="fill_parent"
            android:layout_marginLeft="6dip"
            android:background="@color/black" />

        <LinearLayout
            android:id="@+id/spt_goal_option_ll"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/red_gray_jincai_selector"
                android:orientation="horizontal">

                <CheckedTextView
                    android:id="@+id/bqc_dialog_0_ct"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:background="@drawable/red_gray_jincai_selector"
                    android:gravity="center"
                    android:text="test"
                    android:textColor="@drawable/childtext_selector" />

                <View
                    android:layout_width="0.5dip"
                    android:layout_height="fill_parent"
                    android:background="@color/black" />

                <CheckedTextView
                    android:id="@+id/bqc_dialog_1_ct"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:background="@drawable/red_gray_jincai_selector"
                    android:clickable="false"
                    android:gravity="center"
                    android:textColor="@drawable/childtext_selector" />

                <View
                    android:layout_width="0.5dip"
                    android:layout_height="fill_parent"
                    android:background="@color/black" />

                <CheckedTextView
                    android:id="@+id/bqc_dialog_2_ct"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:background="@drawable/red_gray_jincai_selector"
                    android:button="@null"
                    android:gravity="center"
                    android:textColor="@drawable/childtext_selector" />

                <View
                    android:layout_width="0.5dip"
                    android:layout_height="fill_parent"
                    android:background="@color/black" />

                <CheckedTextView
                    android:id="@+id/bqc_dialog_3_ct"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:background="@drawable/red_gray_jincai_selector"
                    android:button="@null"
                    android:gravity="center"
                    android:textColor="@drawable/childtext_selector" />

                <View
                    android:layout_width="0.5dip"
                    android:layout_height="fill_parent"
                    android:background="@color/black" />

                <CheckedTextView
                    android:id="@+id/bqc_dialog_4_ct"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:background="@drawable/red_gray_jincai_selector"
                    android:button="@null"
                    android:gravity="center"
                    android:textColor="@drawable/childtext_selector" />

                <View
                    android:layout_width="0.5dip"
                    android:layout_height="fill_parent"
                    android:background="@color/black" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <shlottery.gov.cn.lotterycenter.ui.widget.BorderTextview
                            android:id="@+id/bqc_dialog_5_ct"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:background="@drawable/red_gray_jincai_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:textColor="@drawable/childtext_selector" />

                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="fill_parent"
                            android:background="@color/black" />

                        <shlottery.gov.cn.lotterycenter.ui.widget.BorderTextview
                            android:id="@+id/bqc_dialog_6_ct"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:background="@drawable/red_gray_jincai_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:textColor="@drawable/childtext_selector" />

                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="fill_parent"
                            android:background="@color/black" />

                        <shlottery.gov.cn.lotterycenter.ui.widget.BorderTextview
                            android:id="@+id/bqc_dialog_7_ct"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:background="@drawable/red_gray_jincai_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:textColor="@drawable/childtext_selector" />

                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="fill_parent"
                            android:background="@color/black" />

                        <shlottery.gov.cn.lotterycenter.ui.widget.BorderTextview
                            android:id="@+id/bqc_dialog_8_ct"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0"
                            android:background="@drawable/red_gray_jincai_selector"
                            android:button="@null"
                            android:text="test"
                            android:textColor="@drawable/childtext_selector" />

                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="fill_parent"
                            android:background="@color/black" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dip"
                        android:background="@color/black" />
                </LinearLayout>

                <View
                    android:layout_width="0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="1" />


            </LinearLayout>


        </LinearLayout>
    </LinearLayout>

    <include layout="@layout/basecalcel_submit1"></include>
</LinearLayout>
