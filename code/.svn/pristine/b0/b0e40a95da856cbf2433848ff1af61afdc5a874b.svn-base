<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tablerow_white_bg">

    <TextView
        android:id="@+id/member_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:gravity="center"
        android:text=" "
        android:textColor="@color/white"
        android:textSize="@dimen/standard_textsize_f5" />

    <TextView
        android:id="@+id/left_container_textview0"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_size_30dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/dp_size_8dp"
        android:layout_toRightOf="@+id/member_tv"
        android:gravity="center"
        android:maxLines="1"
        android:textSize="@dimen/sp_size_13sp_T13" />

</RelativeLayout>
