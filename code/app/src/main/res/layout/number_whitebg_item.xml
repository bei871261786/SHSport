<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white">

    <TextView
        android:id="@+id/syxw_num_tv"
        style="@style/numlottery_textstyle"
        android:layout_width="@dimen/key_ball_big_width"
        android:layout_height="@dimen/key_ball_big_height" />

    <TextView
        android:id="@+id/miss_num_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/syxw_num_tv"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:singleLine="true"
        android:textColor="@drawable/text_gray_red_selector"
        android:textSize="@dimen/sp_size_12sp_T12" />

</RelativeLayout>