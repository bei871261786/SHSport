<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/white" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1dp" android:color="@color/homeblue"></stroke>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <!--<solid android:color="@color/standard_textcolor_c6" />-->
            <stroke android:color="@color/black_gray1" android:width="1dp"/>
            <solid android:color="@color/gray_bglight" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>