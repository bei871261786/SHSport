<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/coener_radiues" />
            <solid android:color="@color/transparent_half" />
            <stroke android:width="1.0dip" android:color="@color/transparent_half" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/coener_radiues" />
            <solid android:color="@color/white" />
            <stroke android:width="1.0dip" android:color="@color/yellow" />
        </shape>
    </item>
</selector>